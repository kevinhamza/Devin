# .env - Environment variables for Devin Project

# General Project Configuration
APP_NAME=DevinAI
ENVIRONMENT=development
DEBUG_MODE=true

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=devin_db
DB_USER=admin
DB_PASSWORD=securepassword

# AI and API Keys
OPENAI_API_KEY=your_openai_api_key
GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key
AZURE_API_KEY=your_azure_api_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_ACCESS_KEY_ID=your_aws_access_key
GOOGLE_API_KEY=your_google_api_key
GIMINI_API_KEY=your_gimini_api_key


# Security Keys
JWT_SECRET_KEY=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Email Service Configuration
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@example.com
EMAIL_PASSWORD=your_email_password

# Social Media API Tokens
TWITTER_API_KEY=your_twitter_api_key
FACEBOOK_APP_SECRET=your_facebook_app_secret
INSTAGRAM_API_TOKEN=your_instagram_api_token

# Monitoring and Analytics
ANALYTICS_API_KEY=your_analytics_api_key
MONITORING_SERVICE_TOKEN=your_monitoring_service_token

# Cloud Storage Config
CLOUD_STORAGE_BUCKET_NAME=your_bucket_name
CLOUD_STORAGE_ACCESS_KEY=your_access_key
CLOUD_STORAGE_SECRET_KEY=your_secret_key

# Miscellaneous
LOG_LEVEL=info
TIMEZONE=UTC
